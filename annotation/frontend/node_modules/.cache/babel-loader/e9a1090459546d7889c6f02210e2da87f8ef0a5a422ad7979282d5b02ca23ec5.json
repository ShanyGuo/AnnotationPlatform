{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { MessageBox } from 'element-ui';\nexport default {\n  data() {\n    return {\n      stream: \"\",\n      items: []\n    };\n  },\n  mounted() {\n    this.fetchData(); // Call the fetchData method when the component is mounted\n  },\n  methods: {\n    fetchData() {\n      const cachedData = localStorage.getItem('cachedData');\n      if (cachedData) {\n        this.items = JSON.parse(cachedData);\n      } else {\n        // Replace with your JSON file or API endpoint\n        const url = 'http://localhost:8080/bucket/downloadObj';\n\n        // Use axios to fetch data\n        axios.get(url).then(response => {\n          let fileName = response.data.fileName;\n          if (fileName.startsWith(\"mandarin_\")) {\n            this.stream = \"Mandarin\";\n          } else {\n            this.stream = \"Cantonese\";\n          }\n          let fileContent = response.data.fileContent;\n          if (typeof fileContent === 'string') {\n            try {\n              fileContent = JSON.parse(fileContent);\n            } catch (error) {\n              console.error('Error parsing JSON:', error);\n              return; // Exit early if parsing fails\n            }\n          }\n          this.items = fileContent;\n        }).catch(error => {\n          console.error('Error fetching data:', error);\n        });\n      }\n    },\n    submitRatings() {\n      const invalidItems = this.items.filter(item => item.rating === -1);\n      if (invalidItems.length > 0) {\n        MessageBox.alert('All items must be rated before submitting.', 'Error', {\n          confirmButtonText: 'OK',\n          showClose: false,\n          type: 'warning'\n        });\n        return;\n      } else {\n        console.log(this.items);\n        this.$router.push('/thank-you');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","MessageBox","data","stream","items","mounted","fetchData","methods","cachedData","localStorage","getItem","JSON","parse","url","get","then","response","fileName","startsWith","fileContent","error","console","catch","submitRatings","invalidItems","filter","item","rating","length","alert","confirmButtonText","showClose","type","log","$router","push"],"sources":["src/components/RatingPage.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"instruction-area\">\n      <h2>{{stream}} Words Complexity Rating</h2>\n      <p>Instructions for using the rating system:</p>\n      <ul>\n        <li>Rate each item from 1 to 5 stars. 1 star means very easy to understand the keyword in the given sentence, 5 stars means hard to understand the keyword in the given sentence.</li>\n        <li>The \"Keywords\" column indicates the word that needs to be rated, the \"Index\" column shows the position of the keyword in the sentence, and the \"Sentence\" column provides the context sentence around the keyword.</li>\n        <li>Scroll down and make sure to rate all samples before submitting.</li>\n        <li>All samples MUST be rated in one sitting, which takes about 1 hour, without breaks in this process.</li>\n        <li>Click the submit button to send your ratings.</li>\n      </ul>\n    </div>\n    <div class=\"content-area\">\n      <el-table :data=\"items\" style=\"width: 100%\" height=\"775\" :header-cell-style=\"{'background-color': '#f5f5f5'}\" border>\n        <el-table-column prop=\"keywords\" label=\"Keywords\" width=\"120\">\n        </el-table-column>\n        <el-table-column prop=\"index\" label=\"Index\" width=\"120\">\n        </el-table-column>\n        <el-table-column prop=\"sentence\" label=\"Sentence\" width=\"760\">\n        </el-table-column>\n        <el-table-column label=\"Complexity Rating\" width=\"180\">\n          <template slot-scope=\"scope\">\n            <el-rate v-model=\"scope.row.rating\" :max=\"5\" @change=\"handleRatingChange(scope.row)\"></el-rate>\n          </template>\n        </el-table-column>\n      </el-table>\n      <div class=\"button-container\">\n        <el-button type=\"primary\" @click=\"submitRatings\">Submit Ratings</el-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { MessageBox } from 'element-ui';\n\nexport default {\n  data() {\n    return {\n      stream: \"\",\n      items: [],\n    };\n  },\n  mounted() {\n    this.fetchData(); // Call the fetchData method when the component is mounted\n  },\n  methods: {\n    fetchData() {\n      const cachedData = localStorage.getItem('cachedData');\n      if (cachedData) {\n        this.items = JSON.parse(cachedData);\n      } else {\n        // Replace with your JSON file or API endpoint\n      const url = 'http://localhost:8080/bucket/downloadObj';\n\n      // Use axios to fetch data\n      axios.get(url)\n        .then(response => {\n          let fileName = response.data.fileName;\n          if (fileName.startsWith(\"mandarin_\")) {\n            this.stream = \"Mandarin\"\n          } else {\n            this.stream = \"Cantonese\"\n          }\n          \n          let fileContent = response.data.fileContent;\n            if (typeof fileContent === 'string') {\n              try {\n                fileContent = JSON.parse(fileContent);\n              } catch (error) {\n                console.error('Error parsing JSON:', error);\n                return; // Exit early if parsing fails\n              }\n            }\n            this.items = fileContent;\n        })\n        .catch(error => {\n          console.error('Error fetching data:', error);\n        });\n      }\n\n    },\n    submitRatings() {\n      const invalidItems = this.items.filter(item => item.rating === -1);\n        if (invalidItems.length > 0) {\n          MessageBox.alert('All items must be rated before submitting.', 'Error', {\n            confirmButtonText: 'OK',\n            showClose: false,\n            type: 'warning'\n          });\n          return;\n        } else {\n            console.log(this.items);\n            this.$router.push('/thank-you')\n        }\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n.container {\n  display: flex;\n  height: 100vh;\n}\n\n.instruction-area {\n  width: 250px;\n  padding: 20px;\n  background-color: #f5f5f5;\n  overflow-y: auto; /* To allow scrolling if instructions are too long */\n  position: fixed; /* Fixes the instruction area */\n  height: 100%; /* Full height */\n}\n\n.content-area {\n  margin-left: 300px; /* Same width as the instruction area + some gap */\n  //padding: 20px;\n  overflow-y: auto; /* Allows scrolling of the table content */\n  position: fixed;\n  flex-grow: 1;\n}\n\n.button-container {\n  display: flex;\n  justify-content: center;\n  width: 100%;\n}\n\n.el-table {\n  width: 100%;\n}\n\n.el-button {\n  margin-top: 20px;\n}\n\n</style>\n"],"mappings":";AAmCA,OAAAA,KAAA;AACA,SAAAC,UAAA;AAEA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAA;MACA,MAAAE,UAAA,GAAAC,YAAA,CAAAC,OAAA;MACA,IAAAF,UAAA;QACA,KAAAJ,KAAA,GAAAO,IAAA,CAAAC,KAAA,CAAAJ,UAAA;MACA;QACA;QACA,MAAAK,GAAA;;QAEA;QACAb,KAAA,CAAAc,GAAA,CAAAD,GAAA,EACAE,IAAA,CAAAC,QAAA;UACA,IAAAC,QAAA,GAAAD,QAAA,CAAAd,IAAA,CAAAe,QAAA;UACA,IAAAA,QAAA,CAAAC,UAAA;YACA,KAAAf,MAAA;UACA;YACA,KAAAA,MAAA;UACA;UAEA,IAAAgB,WAAA,GAAAH,QAAA,CAAAd,IAAA,CAAAiB,WAAA;UACA,WAAAA,WAAA;YACA;cACAA,WAAA,GAAAR,IAAA,CAAAC,KAAA,CAAAO,WAAA;YACA,SAAAC,KAAA;cACAC,OAAA,CAAAD,KAAA,wBAAAA,KAAA;cACA;YACA;UACA;UACA,KAAAhB,KAAA,GAAAe,WAAA;QACA,GACAG,KAAA,CAAAF,KAAA;UACAC,OAAA,CAAAD,KAAA,yBAAAA,KAAA;QACA;MACA;IAEA;IACAG,cAAA;MACA,MAAAC,YAAA,QAAApB,KAAA,CAAAqB,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,MAAA;MACA,IAAAH,YAAA,CAAAI,MAAA;QACA3B,UAAA,CAAA4B,KAAA;UACAC,iBAAA;UACAC,SAAA;UACAC,IAAA;QACA;QACA;MACA;QACAX,OAAA,CAAAY,GAAA,MAAA7B,KAAA;QACA,KAAA8B,OAAA,CAAAC,IAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}